{
  "request": {
    "method": "POST",
    "url": "http://localhost:9999?delay=200ms",
    "queryParams": {
      "fib": "30"
    },
    "header": {
      "key0": ["val0", "val1"],
      "key1": ["val0"]
    },
    "body": {
      "type": "raw",
      "content": "{\"key0\":\"val0\",\"key1\":\"val1\"}"
    }
  },
  "runner": {
    "requests": 100,
    "concurrency": 1,
    "interval": "50ms",
    "requestTimeout": "2s",
    "globalTimeout": "60s"
  },
  "output": {
    "silent": true
  },
  "tests": [
    {
      "name": "minimum response time",
      "field": "ResponseTimes.Min",
      "predicate": "GT",
      "target": "80ms"
    },
    {
      "name": "maximum response time",
      "field": "ResponseTimes.Max",
      "predicate": "LTE",
      "target": "120ms"
    },
    {
      "name": "100% availability",
      "field": "RequestFailureCount",
      "predicate": "EQ",
      "target": "0"
    }
  ]
}
