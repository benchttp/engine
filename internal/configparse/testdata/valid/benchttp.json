{
  "request": {
    "method": "POST",
    "url": "http://localhost:9999?delay=200ms",
    "queryParams": {
      "fib": "30"
    },
    "header": {
      "key0": ["val0", "val1"],
      "key1": ["val0"]
    },
    "body": {
      "type": "raw",
      "content": "{\"key0\":\"val0\",\"key1\":\"val1\"}"
